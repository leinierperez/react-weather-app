{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": ["react"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  }
}
